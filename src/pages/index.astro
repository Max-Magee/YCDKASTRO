---
import Layout from "../layouts/Layout.astro";
import ThreeCard from "../layouts/ThreeCard.astro";

import cardone from "../assets/group_21.png";
import cardtwo from "../assets/group_22.png";
import cardthree from "../assets/group_23.png";
import BC1 from "../assets/group_24.png";
import BC2 from "../assets/group_25.png";
import AAb from "../assets/AAb.svg"
import biglogo from "../assets/YCDK_logo_big.svg"
import bg1 from "../assets/placeholder.jpg"
import work from "/videos/Guy_Works.mp4"

import BigCardL from "../components/BigCardL.astro";
import BigCardR from "../components/BigCardR.astro";
import HeroImg from "../components/HeroImg.astro";
import HeroVideo from "../components/HeroVideo.astro";
import Graphics from "../components/Graphics.astro";
import Card from "../components/Card.astro";

---

<Layout title="YCDK AtA">

	<HeroImg biglogo={biglogo} bgImage={bg1}/>
 
  <ThreeCard>
    <Card
    placeholder={cardone}
    overskrift="Tester 1"
    tekst="Lorem ipsum dolor sit amet consectetur."/>
    <Card
    placeholder={cardtwo}
    overskrift="Tester 2"
    tekst="Lorem ipsum dolor sit amet consectetur."/>
    <Card
    placeholder={cardthree}
    overskrift="Tester 3"
    tekst="Lorem ipsum dolor sit amet consectetur."/>
  </ThreeCard>

	<HeroVideo biglogo={AAb} herovideo={work}/>
   
  <BigCardL
    placeholder={BC1}
    overskrift="Lorem Ipsum"
    tekst="Lorem ipsum dolor sit amet consectetur. Quis senectus felis
        turpis lorem congue feugiat sagittis dictum." buttonText="Knap 1" />
  <BigCardR
    placeholder={BC2}
    overskrift="Lorem Ipsum"
    tekst="Lorem ipsum dolor sit amet consectetur. Quis senectus felis
        turpis lorem congue feugiat sagittis dictum." buttonText="Knap 2" />

  <Graphics
    herovideo={work}
    overskrift="Lorem Ipsum"/  > 

</Layout>